@{
}
@model MyFreeFormForm.Models.StaticFormModel

<form asp-action="SubmitStaticForm" method="post">
    <div>
        <label asp-for="CompanyName">Company Name</label>
    </div>
    <div>
        <label asp-for="ContactFirstName">First Name</label>
        <input asp-for="ContactFirstName" class="form-control" />
    </div>
    <div>
        <label asp-for="ContactLastName">Last Name</label>
        <input asp-for="ContactLastName" class="form-control" />
    </div>
    <button type="submit" class="btn btn-primary">Submit</button>
</form>
